<!DOCTYPE html>
<html>
<head>
<meta charset="utf-8">
<meta http-equiv="refresh" content="300; url=/">
<meta name="viewport" content="width=device-width, initial-scale=1">
<title><%= title%></title>
<%- include include_style.ejs %>
</head>

<body>
	<nav class="navbar navbar-inverse navbar-fixed-top" role="navigation">
		<div class="container-fluid">
			<div class="navbar-header">
				<button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#sidebar-collapse">
					<span class="sr-only">Toggle navigation</span>
					<span class="icon-bar"></span>
					<span class="icon-bar"></span>
					<span class="icon-bar"></span>
				</button>
				<a class="navbar-brand" href="\"><span>Node</span>Admin</a>
				<% if(user){ %>
					<ul class="user-menu">
						<li class="dropdown pull-right">
							<a href="#" class="dropdown-toggle" data-toggle="dropdown"><span class="fa fa-user"></span>
								<% if(user.name){ %>
									<%= user.name %>
								<% } %>
							<span class="caret"></span></a>
							<ul class="dropdown-menu" role="menu">
								<li><a href="/logout"><span class="fa fa-hand-paper-o"></span> Logout</a></li>
							</ul>
						</li>
				    </ul>
				<% } %>

			</div>
		</div><!-- /.container-fluid -->
	</nav>

	<div id="sidebar-collapse" class="col-sm-3 col-lg-2 sidebar">
		<!--<form role="search">
			<div class="form-group">
				<input type="text" class="form-control" placeholder="Search">
			</div>
		</form>-->
		<ul class="nav menu">
		<% if(title ==='Index'){ %>
			<li class="active">
		<% }else{ %>
			<li>
		<% } %>
			<a href="/"><span class="fa fa-home"></span>  Data center</a></li>
		<% if(user.level === 0){ %>
			<% if(title ==='Account'){ %>
				<li class="active">
			<% }else{ %>
				<li>
			<% } %>
				<a href="/account"><span class="fa fa-user"></span>  Account</a></li>
		<% } %>
	</div><!--/.sidebar-->ㄇ

<!-- main start -->
	<div class="col-sm-9 col-sm-offset-3 col-lg-10 col-lg-offset-2 main">
		<div class="row">
			<div class="col-lg-12">
				<ol class="breadcrumb">
					<li><a href="#"><span class="fa fa-home"></span></a></li>
					<li class="active"><%= title%></li>
					<li class="active">
					</li>
				</ol>
				<p class=" pull-right">
					<% if(title ==='Index'){ %>
					<% } %>
				</p>
			</div>
			<div class ="col-lg-12">
			    <% if(title ==='Index' && user.enable !== false){ %>
					<div class="col-md-4">
						<div class="col-md-12" style="margin-top: 0px;">
							<label>Start Date</label>
							<input type="text" id="date" name="date" size="10"  readonly>
							<input type="button" value="..." id="BTN" name="BTN">
						</div>
					</div>
					<!-- Select Range -->
					<div class="col-md-8">
						<div class="form-group">
						<label>Select Range</label>
						<select "selected_time" id="time_option" name="time_option">
							<option value="volvo" disabled>-- Date range --</option>
							<option value="0" selected>One Day</option>
							<option value="1">One Week</option>
							<option value="2">One Month</option>
						</select>
						</div> <!-- form-group -->
					</div> <!-- col-md-8-->
				<% } %>