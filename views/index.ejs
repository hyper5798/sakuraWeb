                    <div class="top-bg">
					</div>
					<% if(user.enable === false) { %>
						<div class="alert alert-danger" role="alert">
						  	<strong>Waring!</strong> <a href="#" class="alert-link"> Your account is not enabled!</a> Please announce the system administrator.
						</div>
                    <% } else { %>
						<table id="table1"  class="display" cellspacing="0" width="100%">
							<thead>
								<tr style="color:#428bca">
									<th>MAC Address</th>
									<th>Device ID</th>
									<th>Date</th>
									<th>Time</th>
									<th>Service ID</th>
									<th>Service Name</th>
									<th>Package Length</th>
									<th>Byte 0</th>
									<th>Byte 1</th>
								</tr>
							</thead>
						</table>

					<% } %>
					<!-- （Modal） -->
					<div class="modal fade" id="myModal" tabindex="-1" role="dialog"
						aria-labelledby="myModalLabel" aria-hidden="true">
						<div class="modal-dialog">
							<div class="modal-content"  >
								<div class="modal-header " style="background-color:#357ebd;">
									<button type="button" class="close"
									data-dismiss="modal" aria-hidden="true">
										&times;
									</button>
									<h3 class="modal-title" id="myModalLabel">
										<p style="color:white">Option</p>
									</h3>
								</div>

								<div class="modal-body">
									<label>&nbsp;Mac &nbsp;:</label>
									<select name="mac" class="selectpicker" id="mac">
										<option value="1c497b48bb40">Search...</option>
									</select><br><hr>
									<label>From :</label>
									<input type="text" id="startDate" name="startDate" size="10" onclick="btn()" readonly>
									<input id="time1" type="text" class="time" size="8" value="00:00:00" onclick="hideCal()" />
									<label> &nbsp;&nbsp;&nbsp;To :</label>
									<input type="text" id="endDate" name="endDate" size="10" onclick="btn2()" readonly>
									<input id="time2" type="text" class="time" size="8" value="00:00:00" onclick="hideCal()"/>
								</div>
								<div class="modal-footer">
									<button type="button"  onClick="closeDialog()" class="btn btn-default"
										>Close
									</button>
									<button type="button" onClick="firstQuery()" class="btn btn-primary">
										Query
									</button>
								</div>
							</div><!-- /.modal-content -->
					</div><!-- /.modal-dialog -->
<%- include page/footer %>